            marketing_source_name = '' and marketing_source_medium = '' and marketing_source_campaign = ''
                and marketing_source_term = '' and content = ''                                                      as direct_condition,
            
            match(marketing_source_name, '^(google|bing)$')
                and match(marketing_source_medium, '^(.*cp.*|ppc|paid.*)$')                                  as search_paid_condition,
            
            match(marketing_source_name, '^(twitter|facebook|fb|instagram|ig|linkedin|pinterest)$')
                and match(marketing_source_medium, '^(.*cp.*|ppc|paid.*|social_paid)$')                      as social_paid_condition,
            
            match(marketing_source_medium, '^(display|banner|expandable|interstitial|cpm)$')                 as display_condition,
            
            match(marketing_source_medium, '^(.*cp.*|ppc|paid.*)$')                                          as other_paid_condition,
            
            match(marketing_source_name, concat('^.*(twitter|^t\.co|facebook|instagram|LinkedIn|',
                'linkedin|lnkd\.in|pinterest).*'))
                or match(marketing_source_medium, concat('^(social|social_network',
                '|social-network|social_media|social-media|sm)$'))                                  as social_organic_condition,
            
            match(marketing_source_name, '^(google|bing|yahoo|baidu|duckduckgo|yandex|ask)$')
                or marketing_source_medium = 'organic'                                                       as search_organic_condition,
            
            match(marketing_source_name, '^(email|mail|e-mail|e_mail|e mail|mail\.google\.com)$')
                or match(marketing_source_medium, '^(email|mail|e-mail|e_mail|e mail)$')                     as email_condition,
            
            match(marketing_source_name, '(g2|sourceforge|capterra)')                                      as listings_condition,
            
            marketing_source_medium = 'referral'                                                             as referral_condition



marketing_source_name
marketing_source_medium
marketing_source_campaign
marketing_source_marketing_source_term